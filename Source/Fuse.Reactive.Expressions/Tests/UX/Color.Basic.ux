<Panel ux:Class="UX.Color.Basic">
	<LetFloat4 Value="0.5,0.2,0.2,1" ux:Name="c"/>

	<FuseTest.DudElement Float4Value="darken({c},0.1)" ux:Name="f1"/>
	<FuseTest.DudElement Float4Value="lighten({c},0.1)" ux:Name="f2"/>
	<FuseTest.DudElement Float4Value="desaturate({c},0.1)" ux:Name="f3"/>
	<FuseTest.DudElement Float4Value="saturate({c},0.1)" ux:Name="f4"/>
	<FuseTest.DudElement Float4Value="scaleSaturation({c},0.2)" ux:Name="f5"/>
	<FuseTest.DudElement Float4Value="scaleSaturation({c},-0.5)" ux:Name="f6"/>
	<FuseTest.DudElement Float4Value="scaleLightness({c},0.5)" ux:Name="f7"/>
	<FuseTest.DudElement Float4Value="scaleLightness({c},-0.2)" ux:Name="f8"/>
	<FuseTest.DudElement Float4Value="adjustHue({c},0.6)" ux:Name="f9"/>
	<FuseTest.DudElement Float4Value="adjustHue({c},-0.8)" ux:Name="f10"/>

	<FuseTest.DudElement Float4Value="rgbaToHsla({c})" ux:Name="q1"/>
	<FuseTest.DudElement Float4Value="hslaToRgba(rgbaToHsla({c}))" ux:Name="q2"/>
</Panel>