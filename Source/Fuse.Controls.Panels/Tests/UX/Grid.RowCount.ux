<Panel ux:Class="UX.Grid.RowCount">
	<Grid Alignment="Left" RowCount="5" ux:Name="G1" ChildOrder="ColumnMajor">
		<Panel ux:Name="T1" Width="50"/>
		<Panel ux:Name="T2" Width="50"/>
		<Panel ux:Name="T3" Width="50"/>
		<!-- nothing, but the others should still be weight as though there are 5 rows -->
	</Grid>

	<Grid Alignment="Right" RowCount="5" ux:Name="G2" ColumnCount="1">
		<Panel ux:Name="R1" Width="50"/>
		<Panel ux:Name="R2" Width="50"/>
		<Panel ux:Name="R3" Width="50"/>
		<!-- nothing, but the others should still be weight as though there are 5 rows -->
	</Grid>

	<Grid Alignment="Left" ux:Name="G3">
		<Panel ux:Name="S2" Row="2" ux:AutoBind="false"/>
		<Panel ux:Name="S5" Row="4" Width="50"/>
	</Grid>
</Panel>